      box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
      z-index: 1;
    }
        .profile_points{
            font-size: 20px;
            max-height: 25px;
            font-family: "Karla", sans-serif;
            background-color: #FFB428;
        }
        .dropdown_a{
            min-height: 20px;
            padding: 10px;
        }
        .mystyle {
            width: 100%;
            color: var(--white);
            font-size: 25px;
            box-sizing: border-box;
            display: block;
            transition: all 0.5s ease-out;
            top: 0px;
            transition: all 0.5s ease-in-out;
            transform: scale(1);
            z-index: 3;
            left: 0;
            position: absolute;
            top: 0;
            left: 0;
            background: #ffffff;
        }
        .close_nav_berger{
            position: absolute;
            right: 15px;
            top: 15px;
            z-index: 10;
            display: none;
        }
        .open_nav_burger{
            display: none;
        }
        @media (max-width: 920px){
            .close_nav_berger,
            .open_nav_burger{
                display: block;
                cursor: pointer;
            }
        }

        @media (max-width:500px){
    .dropdown-content{
        background-color: white;
    }
    .profile_points{
        background-color: white;
    }
}

