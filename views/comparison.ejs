var user_s = [];

async function fetchUsersFromDatabase() {
    const currentURL = window.location.href;
    const segments = currentURL.split('/');
    const competitionName = segments[segments.length - 1];

    try {
        const response = await fetch(`/competitionUsers/${competitionName}`);
        if (!response.ok) {
            throw new Error(`HTTP error! Status: ${response.status}`);
        }
        const data = await response.json();
        console.log("Competition Data: ", data);
        user_s =  data;
    } catch (error) {
        
        console.error(error);
        throw error;
    }
}


async function fetchData() {
    try {
         await fetchUsersFromDatabase();
         changePage(1, user_s);
        console.log(user_s);
        
    } catch (error) {
        console.error(error);
    }
}




function prevPage()
{
    if (current_page > 1) {
        current_page--;
        changePage(current_page, user_s);
    }
}

function nextPage()
{
    if (current_page < numPages()) {
        current_page++;
        changePage(current_page, user_s);
    }
}

function changePage(page, user_s)
{
    var btn_next = document.getElementById("btn_next");
    var btn_prev = document.getElementById("btn_prev");
    var listing_table = document.getElementById("listingTable");
    var page_span = document.getElementById("page");

    // Validate page
    if (page < 1) page = 1;
    if (page > numPages()) page = numPages();
    listing_table.innerHTML = "";

    

    for (var i = (page-1) * records_per_page; i < (page * records_per_page); i++) {
        const ad = { adName: 
        `
            <div class="competition_user_row r-flex ali-c jut-sb">
                <li class="t_data competition_user_box r-flex ali-c">
                    
                    <span class="competition_user_img r-flex ali-c jut-c">
                        <img src="../images/logo.png">
                    </span>
                    <span class="competition_user_name">${user_s[i].name}</span>
                </li>
                <li class="t_data">ğŸ† &nbsp; ${user_s[i].rank}</li>
                <li class="t_data">ğŸ“ &nbsp; <a href="/read?blogId=${user_s[i].blogId}">Read</a></li>
            </div>
        `
    }

        listing_table.innerHTML += ad.adName + " ";
    }
    page_span.innerHTML = page;

    if (page == 1) {
        btn_prev.style.visibility = "hidden";
    } else {
        btn_prev.style.visibility = "visible";
    }

    if (page == numPages()) {
        btn_next.style.visibility = "hidden";
    } else {
        btn_next.style.visibility = "visible";
    }
}

function numPages()
{
    return Math.ceil(user_s.length / records_per_page);
}
window.onload = function() {
    fetchData();
